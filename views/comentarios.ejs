<!DOCTYPE html>
<html>
  <head>
    <!--Import Google Icon Font-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!--Import materialize.css-->
    <link type="text/css" rel="stylesheet" href="/css/materialize.min.css"  media="screen,projection"/>
    <link type="text/css" rel="stylesheet" href="/css/bootstrap.min.css"  media="screen,projection"/>
    <link type="text/css" rel="stylesheet" href="/css/bootstrap.min.css.map"  media="screen,projection"/>
    <link type="text/css" rel="stylesheet" href="/css/home.css"  media="screen,projection"/>
    <link type="text/css" rel="stylesheet" href="/css/comum.css"  media="screen,projection"/>
   
    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
  </head>

  <body class=" blue-grey darken-3">
   
    <div class="row m-5">
        <div class="col-4 center" > <img width="100" height="100" src="https://www.google.com/imgres?imgurl=https%3A%2F%2Fimg1.gratispng.com%2F20180411%2Fctq%2Fkisspng-speech-balloon-online-chat-computer-icons-feedback-5acdeda6567d80.7774427615234451583543.jpg&imgrefurl=https%3A%2F%2Fwww.gratispng.com%2Fbaixar%2Fcoment%25C3%25A1rios.html&tbnid=lgn8rSA-A_hIYM&vet=12ahUKEwjAj5eQ2PfvAhW2M7kGHcm6BKQQMygCegUIARC7AQ..i&docid=jzi6lLwkNqe-LM&w=260&h=280&q=bal%C3%A3o%20comentarios%20&client=firefox-b-d&ved=2ahUKEwjAj5eQ2PfvAhW2M7kGHcm6BKQQMygCegUIARC7AQ"> </div>
        <div class="col-6 "><h1>Usuarios</h1></div>

    </div>
    <div class="row">
        <div class="container ">
          Comentarios:
          <% comentarios.forEach(com => { %>
            <tr>
                <td>
                    <%= com.key %>
                </td>
                <td>
                    <%= com.compentario %>
                </td>
            </tr>
            <% }); %>
      </div>
    </div>

    <!--JavaScript at end of body for optimized loading-->
    <script type="text/javascript" src="/js/materialize.min.js"></script>
    <script type="text/javascript" src="/js/jquery.slim.min.js"  ></script>
    <script type="text/javascript" src="/js/popper.min.js"  ></script>
    <script type="text/javascript" src="/js/bootstrap.min.js" ></script>
    <script type="text/javascript" src="/js/axios.js"></script>
    <script type="text/javascript" src="/js/controleUsuarios.js"></script>
    <script>
        $(document).ready(function() {
          M.updateTextFields();
        });
    </script>
  </body>
</html>